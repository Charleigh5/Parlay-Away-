{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "data": [
        "./src/data"
      ],
      "data/*": [
        "./src/data/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "services": [
        "./src/services"
      ],
      "services/*": [
        "./src/services/*"
      ],
      "types": [
        "./src/types.ts"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "utils": [
        "./src/utils.ts"
      ],
      "utils/*": [
        "./src/utils/*"
      ]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}